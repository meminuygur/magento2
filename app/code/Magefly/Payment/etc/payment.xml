<?xml version="1.0" ?>
    <payment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Payment:etc/payment.xsd">
        <groups>
            <group id="mageflypayment_group">
                <label>Magefly Payment</label>
            </group>
        </groups>
        <methods>
            <method name="mageflypayment">
                <allow_multiple_address>1</allow_multiple_address>
            </method>
            <method name="mageflypayment_isbank">
                <allow_multiple_address>1</allow_multiple_address>
            </method>
            <method name="mageflypayment_girogate">
                <allow_multiple_address>1</allow_multiple_address>
            </method>
        </methods>
    </payment>
